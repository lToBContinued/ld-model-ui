{
  "name": "ld-model-ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite --open",
    "test": "vite --mode test",
    "prod": "vite --mode production",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:test": "vite build --mode test",
    "preview": "vite preview --open",
    "type-check": "vue-tsc --build",
    "lint:eslint": "eslint src/**/*.{js,ts,vue} --cache --cache-location node_modules/.cache/.eslintcache --fix",
    "lint:style": "stylelint src/**/*.{css,scss,sass,vue} --cache --cache-location node_modules/.cache/.stylelintcache --fix",
    "format": "prettier --write \"./**/*.{html,vue,ts,js,json,md}\"",
    "prepare": "husky",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,ts,vue}": [
      "eslint src/**/*.{js,ts,vue} --cache --cache-location node_modules/.cache/.eslintcache --fix",
      "prettier --write \"./**/*.{html,vue,ts,js,json,md}\""
    ],
    "src/**/*.{css,scss,sass,vue}": [
      "stylelint src/**/*.{css,scss,sass,vue} --cache --cache-location node_modules/.cache/.stylelintcache --fix"
    ]
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@vueup/vue-quill": "^1.2.0",
    "@vueuse/core": "^13.6.0",
    "axios": "^1.11.0",
    "echarts": "^6.0.0",
    "element-plus": "^2.10.4",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "particles.vue3": "^1.43.1",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.4.1",
    "sheetjs": "^2.0.0",
    "tsparticles-slim": "^2.12.0",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "vue3-ace-editor": "^2.2.4",
    "w-normalize-global-css": "^1.0.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.32.0",
    "@iconify-json/ep": "^1.2.2",
    "@tsconfig/node22": "^22.0.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^22.16.5",
    "@types/nprogress": "^0.2.3",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.13.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "lint-staged": "^16.1.2",
    "npm-run-all2": "^8.0.4",
    "postcss": "^8.0.0",
    "prettier": "3.6.2",
    "sass-embedded": "^1.89.2",
    "stylelint": "^16.11.0",
    "stylelint-config-clean-order": "^6.1.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-prettier": "^5.0.2",
    "typescript": "~5.8.0",
    "typescript-eslint": "^8.38.0",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-icons": "^22.2.0",
    "unplugin-vue-components": "^28.8.0",
    "vite": "^7.0.6",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.4"
  }
}
